{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      /* Modules */
      "rootDir": "src" /* Specify the root folder within your source files. */,
    "target": "ES2020",
    "module": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
    },
    "include": ["src"],
    "typeRoots": ["./node_modules/@types", "./src/types"]
}
